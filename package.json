{
   "name": "nextjs-cors",
   "version": "1.0.2@beta",
   "private": false,
   "description": "nextjs-cors is a node.js package to provide a Connect/Express middleware that can be used to enable CORS with various options.",
   "keywords": [
      "nextjs-cors",
      "express-cors",
      "nextjs-express-cors",
      "next-cors",
      "cors-server",
      "cors"
   ],
   "homepage": "https://github.com/yonycalsin/nextjs-cors#readme",
   "bugs": {
      "url": "https://github.com/yonycalsin/nextjs-cors/issues"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/yonycalsin/nextjs-cors.git"
   },
   "license": "MIT",
   "author": "Yony Calsin",
   "main": ".dist/index.js",
   "types": ".dist/index.d.ts",
   "scripts": {
      "build": "tsc",
      "clean": "rimraf .dist",
      "dev": "nodemon",
      "format": "prettier --write ./**/*.{ts,json}",
      "typecheck": "tsc --noEmit"
   },
   "husky": {
      "hooks": {
         "pre-push": "npm run typecheck"
      }
   },
   "lint-staged": {
      "*.{ts,js}": [
         "eslint --fix"
      ]
   },
   "dependencies": {
      "cors": "^2.8.5"
   },
   "devDependencies": {
      "@types/cors": "^2.8.7",
      "@types/react": "^17.0.2",
      "@types/react-dom": "^17.0.1",
      "husky": "^5.1.1",
      "lint-staged": "^10.5.4",
      "next": "^10.0.7",
      "nodemon": "^2.0.4",
      "prettier": "^2.0.5",
      "prettier-plugin-organize-imports": "^1.1.1",
      "prettier-plugin-packagejson": "^2.2.5",
      "rimraf": "^3.0.2",
      "ts-node": "^8.10.2",
      "typescript": "^3.9.7"
   },
   "peerDependencies": {
      "next": "^8.1.1-canary.54 || ^9.0.0 || ^10.0.0"
   }
}
